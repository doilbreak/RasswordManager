<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="167.0" prefWidth="252.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="com.radilbe.rassword.AddUserDialogController">
    <children>
        <Label layoutX="14" layoutY="14" text="Username:" />
        <TextField fx:id="usernameField" layoutX="90" layoutY="10" />

        <Label layoutX="14" layoutY="54" text="Email:" />
        <TextField fx:id="emailField" layoutX="90" layoutY="50" />

        <Label layoutX="14" layoutY="94" text="Password:" />
        <TextField fx:id="passwordField" layoutX="90" layoutY="90" />

        <Button layoutX="40" layoutY="130" onAction="#saveUser" text="Save" />
        <Button layoutX="120" layoutY="130" onAction="#cancel" text="Cancel" />
    </children>
</AnchorPane>
